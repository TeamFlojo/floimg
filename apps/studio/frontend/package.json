{
  "name": "@teamflojo/floimg-studio-ui",
  "version": "0.2.3",
  "description": "FloImg Studio React components for building visual workflow editors",
  "type": "module",
  "license": "MIT",
  "author": "Flojo, Inc.",
  "homepage": "https://floimg.com/studio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamflojo/floimg.git",
    "directory": "apps/studio/frontend"
  },
  "bugs": {
    "url": "https://github.com/teamflojo/floimg/issues"
  },
  "keywords": [
    "floimg",
    "studio",
    "workflow",
    "image",
    "editor",
    "react",
    "visual"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite 2>&1 | tee ../../logs/frontend.log",
    "build": "vite build --mode lib",
    "build:app": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@tanstack/react-query": "^5.0.0",
    "reactflow": "^11.0.0",
    "zustand": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@teamflojo/floimg-studio-shared": "^0.1.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.90.12",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "reactflow": "^11.11.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "zustand": "^5.0.9"
  }
}
