{
  "name": "@teamflojo/floimg-qr",
  "version": "0.2.1",
  "description": "QR code generator for floimg using qrcode library",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "floimg",
    "qr",
    "qrcode",
    "barcode",
    "image-generation"
  ],
  "author": "Brett Cooke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TeamFlojo/floimg.git",
    "directory": "packages/floimg-qr"
  },
  "peerDependencies": {
    "@teamflojo/floimg": "^0.2.0"
  },
  "dependencies": {
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/qrcode": "^1.5.5",
    "@teamflojo/floimg": "workspace:*",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
