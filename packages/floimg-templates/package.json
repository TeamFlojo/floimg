{
  "name": "@teamflojo/floimg-templates",
  "version": "0.2.0",
  "description": "Official workflow templates for FloImg Studio",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "floimg",
    "templates",
    "workflows",
    "image-generation"
  ],
  "author": "Brett Cooke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TeamFlojo/floimg.git",
    "directory": "packages/floimg-templates"
  },
  "dependencies": {
    "@teamflojo/floimg-studio-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
